msgid "The maximum time allowed for a rendering time slice"
msgstr "Ο μέγιστος επιτρεπόμενος χρόνος για απόδοση αποσπάσματος"

#: ../src/ui/dialog/inkscape-preferences.cpp:3036
msgid "Persistent"
msgstr "Επίμονο"

#: ../src/ui/dialog/inkscape-preferences.cpp:3036
msgid "Asynchronous"
msgstr "Ασύγχρονο"

#: ../src/ui/dialog/inkscape-preferences.cpp:3036
